<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">
    <transfer name="AddCandidateRequest">
        <property name="quote" type="Quote"/>
        <property name="voucherCode" type="string"/>
    </transfer>

    <transfer name="RestDiscountsRequestAttributes">
        <property name="code" type="string"/>
    </transfer>

    <transfer name="RestDiscountsAttributes">
        <property name="amount" type="int"/>
        <property name="netAmount" type="int"/>
        <property name="code" type="string"/>
        <property name="discountType" type="string"/>
        <property name="displayName" type="string"/>
        <property name="isExclusive" type="boolean"/>
        <property name="expirationDateTime" type="TIMESTAMP"/>
        <property name="discountPromotionAbstractSku" type="string"/>
        <property name="discountPromotionQuantity" type="int"/>
    </transfer>

</transfers>
